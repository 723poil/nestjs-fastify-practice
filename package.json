{
  "name": "nestjs-fastify-practice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:markdown": "npx prisma generate",
    "nestia:swagger": "npx nestia swagger",
    "nestia:all": "npx nestia all"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@fxts/core": "^1.15.0",
    "@nestia/core": "^7.3.1",
    "@nestia/e2e": "^7.3.1",
    "@nestia/fetcher": "^7.3.1",
    "@nestia/sdk": "^7.3.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.13.0",
    "dayjs": "^1.11.13",
    "fastify": "^5.4.0",
    "nest-winston": "^1.10.2",
    "prisma-markdown": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typia": "^9.6.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^10.0.0",
    "@nestia/benchmark": "^7.3.1",
    "@nestjs/cli": "^11.0.8",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nestia": "^7.3.1",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "prisma"
  }
}